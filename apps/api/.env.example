# API
NODE_ENV=development
PORT=4000
CORS_ORIGINS="http://localhost:8081"

# PostgreSQL Settings
DATABASE_URL=postgresql://admin:admin123@localhost:5432/appdb

POSTGRES_CONTAINER_NAME=app-postgres
POSTGRES_DB=appdb
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin123
POSTGRES_PORT=5432

# pgAdmin Settings
PGADMIN_CONTAINER_NAME=app-pgadmin
PGADMIN_DEFAULT_EMAIL=admin@binaryexports.com
PGADMIN_DEFAULT_PASSWORD=admin123
PGADMIN_PORT=5050

# Logging
LOG_LEVELS=All
ROLLBAR_LOG_LEVELS=All

# Rollbar
ROLLBAR_ACCESS_TOKEN=your-token-here

# Better Auth
BETTER_AUTH_TRUSTED_ORIGINS=http://localhost:3000,http://localhost:8081
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Email (Resend)
RESEND_API_KEY=re_your_resend_api_key
RESEND_FROM_EMAIL=noreply@yourdomain.com
