# API
NODE_ENV=development
PORT=4000
CORS_ORIGINS=http://localhost:*,http://*.localhost:*,https://binaryexports.com,https://*.binaryexports.com,https://binaryexploits.com,https://*.binaryexploits.com,https://binaryexperiments.com,https://*.binaryexperiments.com

# Super admin: comma-separated emails that can manage tenants (tenant.* tRPC)
SUPER_ADMIN_EMAILS=anns.shahbaz@binaryexports.com,anns.shahbaz@binaryexperiments.com,anns.shahbaz@binaryexploits.com

# PostgreSQL
POSTGRES_CONTAINER_NAME=app-postgres
POSTGRES_DB=appdb
POSTGRES_USER=admin
POSTGRES_PASSWORD=admin123
POSTGRES_PORT=5432
DATABASE_URL_POSTGRES=postgresql://admin:admin123@localhost:5432/appdb

# pgAdmin
PGADMIN_CONTAINER_NAME=app-pgadmin
PGADMIN_DEFAULT_EMAIL=admin@admin.com
PGADMIN_DEFAULT_PASSWORD=admin123
PGADMIN_PORT=5050

# MongoDB
MONGODB_CONTAINER_NAME=app-mongodb
MONGODB_PORT=27017
MONGODB_USER=admin
MONGODB_PASSWORD=admin123
MONGODB_REPLICA_SET_KEY=your_key_here
DATABASE_URL_MONGODB=mongodb://admin:admin123@localhost:27017/appdb?authSource=admin&replicaSet=rs0&directConnection=true

# Mongo Express
MONGO_EXPRESS_CONTAINER_NAME=app-mongo-express
MONGO_EXPRESS_USER=admin
MONGO_EXPRESS_PASSWORD=admin123
MONGO_EXPRESS_PORT=8100

# Logging
LOG_LEVELS=All
ROLLBAR_LOG_LEVELS=Critical

# Rollbar
ROLLBAR_ACCESS_TOKEN=your_token_here

# Better Auth (trusted origins must include all tenant frontend URLs for auth/cookies)
BETTER_AUTH_SECRET=your_secret_here
BETTER_AUTH_URL=http://localhost:4000
BETTER_AUTH_TRUSTED_ORIGINS=http://localhost:*,http://*.localhost:*,https://binaryexports.com,https://*.binaryexports.com,https://binaryexploits.com,https://*.binaryexploits.com,https://binaryexperiments.com,https://*.binaryexperiments.com,mobile://,exp://
GOOGLE_CLIENT_ID=your_client_id_here
GOOGLE_CLIENT_SECRET=your_client_secret_here

# Email (Resend)
RESEND_API_KEY=re_your_api_key_here
RESEND_FROM_EMAIL=onboarding@resend.dev