  },
  "devDependencies": {
    "@repo/utils-core": "workspace:*",
    "@repo/db-seeder": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "^9.34.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}