  "private": true,
  "main": "./generated/prisma/index.js",
  "types": "./generated/prisma/index.d.ts",
  "prisma": {
    "seed": "ts-node --transpile-only --compiler-options {\"module\":\"CommonJS\"} ./seed.ts"
