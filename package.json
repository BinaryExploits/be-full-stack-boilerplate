{
  "name": "be-full-stack-boilerplate",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Binary Exploits LLC",
    "url": "https://www.binaryexploits.com"
  },
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:web": "pnpm --filter web dev",
    "dev:api": "pnpm --filter api dev",
    "dev:mobile": "pnpm --filter mobile dev",
    "build": "turbo run build",
    "build:web": "pnpm --filter web build",
    "build:api": "pnpm --filter api build",
    "build:mobile:web": "echo Todo",
    "build:mobile:android": "echo Todo",
    "build:mobile:ios": "echo Todo",
    "start": "turbo run start --parallel",
    "start:web": "pnpm --filter web start",
    "start:api": "pnpm --filter api start:prod",
    "start:mobile": "echo Todo",
    "start:mobile:web": "echo Todo",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "check-types": "turbo run check-types",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rimraf node_modules .turbo && pnpm recursive exec rimraf dist .next web-build"
  },
  "devDependencies": {
    "@tanstack/react-query": "5.76.1",
    "@trpc/client": "11.1.2",
    "@trpc/react-query": "11.1.2",
    "@trpc/server": "11.1.2",
    "prettier": "^3.6.2",
    "turbo": "^2.5.8",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
